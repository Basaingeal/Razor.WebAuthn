{
  "name": "currietechnologies-razorwebauthn",
  "version": "0.1.0",
  "description": "A Razor class library for interacting with the browser WebAuthn API.",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p --color",
    "build:tsc": "tsc",
    "watch": "webpack --watch",
    "lint": "eslint ./src --ext .ts",
    "prettier:write": "prettier --write ./src/*.ts",
    "prettier:check": "prettier --check ./src/*.ts",
    "babel": "babel ./src/WebAuthn.ts --out-dir wwwroot --extensions .ts,.tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "prettier": "^1.18.2",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.6.0-beta",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  },
  "author": "Michael J. Currie",
  "private": true,
  "eslintIgnore": [
    "*.js",
    "*.ts"
  ],
  "browserslist": [
    "chrome 67",
    "edge 18",
    "firefox 60",
    "opera 54"
  ],
  "dependencies": {
    "core-js": "^3.1.4"
  }
}
