{
  "compilerOptions": {
    "outDir": "./wwwroot/",
    "sourceMap": false,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "allowJs": true
  },
  "include": ["./src"]
}